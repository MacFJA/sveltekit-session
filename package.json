{
  "name": "@macfja/sveltekit-session",
  "version": "1.0.0",
  "description": "Session management for SvelteKit",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "svelte": "dist/index.mjs",
  "exports": {
    ".": {
      "import": [
        "./dist/index.mjs",
        "./dist/node.mjs",
        "./dist/redis.mjs",
        "./dist/utils.mjs"
      ],
      "require": [
        "./dist/index.cjs",
        "./dist/node.cjs",
        "./dist/redis.cjs",
        "./dist/utils.cjs"
      ]
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "rollup -c",
    "lint": "prettier --plugin-search-dir . --check . && eslint . && tsc --noEmit",
    "format": "prettier --plugin-search-dir . --write .",
    "test:dev": "cd tests && vite dev"
  },
  "devDependencies": {
    "@redis/client": "^1.3.1",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/package": "next",
    "@types/cookie": "^0.5.1",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "rollup": "^3.3.0",
    "rollup-plugin-dts-bundle-generator": "^1.4.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.1.0"
  },
  "type": "module",
  "dependencies": {
    "cookie": "^0.5.0",
    "devalue": "^4.2.0",
    "random-token": "^0.0.8",
    "@sveltejs/kit": "next"
  },
  "files": [
    "dist/",
    "LICENSE.md",
    "README.md"
  ],
  "bugs": {
    "url": "https://github.com/macfja/sveltekit-session/issues"
  },
  "homepage": "https://github.com/macfja/sveltekit-session#readme",
  "author": "MacFJA",
  "license": "MIT",
  "types": "dist/",
  "keywords": [
    "session",
    "svelte",
    "sveltekit",
    "sveltejs"
  ]
}
